<%# Modal button %>
<button type="button" class="btn btn-default btn-sm"
        data-toggle="modal" data-target="#style-explanation">
  Styles Explanation
</button>

<!-- style-explanation modal-->
<div class="modal fade" id="style-explanation" role="dialog">
  <div class="modal-dialog">
    <%# Modal Content %>
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">
          &times;
        </button>
        <h3 align="center">Voting Styles</h3>
      </div>
      <div class="modal-body">
        <table class="table table-striped table-condensed">
          <tr>
            <th>Voting Style Name</th>
            <th colspan=10>Vote Choices</th>
          </tr>
          <tr>
            <th>Binary</th>
            <td>0</td>
            <td>1</td>
            <% 8.times do %>
              <td></td>
            <% end %>
          </tr>
          <tr>
            <th>5 Stars</th>
            <td>1</td>
            <td>2</td>
            <td>3</td>
            <td>4</td>
            <td>5</td>
            <% 5.times do %>
              <td></td>
            <% end %>
          </tr>
          <tr>
            <th>1-10</th>
            <td>1</td>
            <td>2</td>
            <td>3</td>
            <td>4</td>
            <td>5</td>
            <td>6</td>
            <td>7</td>
            <td>8</td>
            <td>9</td>
            <td>10</td>
          </tr>
          <tr>
            <th>Fibonacci</th>
            <td>1</td>
            <td>2</td>
            <td>3</td>
            <td>5</td>
            <td>8</td>
            <td>13</td>
            <td>21</td>
            <% 3.times do %>
              <td></td>
            <% end %>
          </tr>
        </table>
      </div>
    </div>
  </div>
</div><!-- /style-explanation modal -->