<%# Modal button %>
<button type="button" class="btn btn-default btn-sm"
        data-toggle="modal" data-target="#style-explanation">
  Styles Explanation
</button>

<!-- style-explanation modal-->
<div class="modal fade" id="style-explanation" role="dialog">
  <div class="modal-dialog">
    <%# Modal Content %>
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">
          &times;
        </button>
        <h3 align="center">Voting Styles</h3>
      </div>
      <div class="modal-body">
        <table class="table table-striped table-condensed text-center">
          <tr>
            <th>Voting Style Name</th>
            <th class="text-center">Vote Choices</th>
          </tr>
          <tr>
            <th>Binary</th>
            <td>0 or 1</td>
          </tr>
          <tr>
            <th>5 Stars</th>
            <td>1 - 5</td>
          </tr>
          <tr>
            <th>1-10</th>
            <td>1 - 10</td>
          </tr>
          <tr>
            <th>Fibonacci</th>
            <td>1, 2, 3, 5, 8, 13, 21</td>
          </tr>
        </table>
      </div>
    </div>
  </div>
</div><!-- /style-explanation modal -->
